<script>
    $('.county_chnage').change(function(e) {
        e.preventDefault();
        if ($(this).attr('id') != 'permanent_country_id') {
            var stateid = 'comm_state_id';
        } else {
            var stateid = 'permanent_state_id';
        }
        $.ajaxSetup({
            headers: {
                'X-CSRF-TOKEN': $('meta[name="csrf-token"]').attr('content')
            }
        });
        $.ajax({
            url: "{{ route('get-states') }}",
            type: "POST",
            data: {
                id: $(this).val()
            },
            success: function(data) {
                var selOpts = "<option>Select</option>";
                $.each(data, function(k, v) {
                    var id = data[k].id;
                    var val = data[k].state_name;
                    selOpts += "<option value='" + id + "'>" + val + "</option>";
                });
                $('#' + stateid).html(selOpts);

            }
        });

    });

    $('.state_chnage').change(function(e) {
        e.preventDefault();
        if ($(this).attr('id') != 'permanent_state_id') {
            var stateid = 'comm_city_id';
        } else {
            var stateid = 'permanent_city_id';
        }
        $.ajaxSetup({
            headers: {
                'X-CSRF-TOKEN': $('meta[name="csrf-token"]').attr('content')
            }
        });
        $.ajax({
            url: "{{ route('get-city') }}",
            type: "POST",
            data: {
                id: $(this).val()
            },
            success: function(data) {
                var selOpts = "<option>Select</option>";
                $.each(data, function(k, v) {
                    var id = data[k].id;
                    var val = data[k].city_name;
                    selOpts += "<option value='" + id + "'>" + val + "</option>";
                });
                $('#' + stateid).html(selOpts);

            }
        });

    });

    $('.city_chnage').change(function(e) {
