        } else {
            var stateid = 'permanent_address2';
        }
        $.ajaxSetup({
            headers: {
                'X-CSRF-TOKEN': $('meta[name="csrf-token"]').attr('content')
            }
        });
        $.ajax({
            url: "{{ route('get-area') }}",
            type: "POST",
            data: {
                id: $(this).val()
            },
