<?php

namespace App\Models;

use Illuminate\Database\Eloquent\Factories\HasFactory;
use Illuminate\Database\Eloquent\Model;
use Illuminate\Database\Eloquent\SoftDeletes;

class Products extends Model
{
    use HasFactory,SoftDeletes;

    protected $fillable = [
        'product_name',
        'product_type_id',
        'brand_id',
        'category_id',
        'product_image',
        'customer_price',
        'wholesale_price',
        'capacity',
        'desc',
        'is_emptycan_return',
        'CGST',
        'SGST',
        'is_active',
        'created_by',
        'updated_by',
        'deleted_by',
        'created_at',
        'updated_at',
        'deleted_at'
    ];
}
