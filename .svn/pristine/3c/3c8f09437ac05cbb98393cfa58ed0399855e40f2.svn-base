            var stateid = 'ddlCommArea';
        } else {
            var stateid = 'ddlPremArea';
        }
        $.ajaxSetup({
            headers: {
                'X-CSRF-TOKEN': $('meta[name="csrf-token"]').attr('content')
            }
        });
        $.ajax({
            url: "{{ route('get-area') }}",
